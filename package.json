{
  "name": "linter-puppet-lint-for-linter-3",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "Lint puppet on save using puppet-lint",
  "repository": "https://github.com/rafitadiaz/linter-puppet-lint-for-linter-3",
  "license": "MIT",
  "engines": {
    "atom": ">0.180.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-linter": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "atom": true
    }
  }
}
